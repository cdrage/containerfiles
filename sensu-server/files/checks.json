{
  "checks": {
    "cpu_check": {
      "handlers": ["default"],
      "command": "/etc/sensu/plugins/check-cpu.rb -w 90",
      "interval": 120,
      "subscribers": [ "node" ]
    },
    "eth_metrics": {
      "type": "metric",
      "handlers": ["graphite"], 
      "command": "/etc/sensu/plugins/metrics-net.rb --scheme :::name:::.net",
      "interval": 5,
      "subscribers": [ "node" ]
    }
  }
}

